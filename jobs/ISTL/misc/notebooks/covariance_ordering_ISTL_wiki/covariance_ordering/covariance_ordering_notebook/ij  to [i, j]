      ij  [i, j]
      __________
      0 = [0, 0]
      1 = [1, 0]
      2 = [1, 1]
      3 = [2, 0]
      4 = [2, 1]
      5 = [2, 2]
      6 = [3, 0]
      7 = [3, 1]
      8 = [3, 2]
      9 = [3, 3]
      10 = [4, 0]
      ...
      50 = [9, 5]
      51 = [9, 6]
      52 = [9, 7]
      53 = [9, 8]
      54 = [9, 9]        

